# Start 3 Single-node cluster

raftgroup1-1: go run main.go --mode rm --clusterid 1 --id 1 --cluster http://172.24.20.67:12379 --rmport :50041 --db mas1

raftgroup2-1: go run main.go --mode rm --clusterid 2 --id 1 --cluster http://172.24.20.67:22379 --rmport :50051 --db mas2

raftgroup3-1: go run main.go --mode rm --clusterid 3 --id 1 --cluster http://172.24.20.67:32379 --rmport :50061 --db mas3
